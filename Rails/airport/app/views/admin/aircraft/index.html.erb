<h1>Aircraft#index</h1>

<table >
  <tr>
    <td>
      Name
    </td>
    <td>
      Cruising Range
    </td>
    <td colspan = "3">
    Operations
    </td>
  </tr>

<% @aircrafts.each do |aircraft|%>
  <tr>
    <td><%= aircraft.name%></td>
    <td><%= aircraft.cruising_range%></td>
    <td><%= link_to("Show",admin_aircraft_path(aircraft))%></td>
    <td><%= link_to("Update", edit_admin_aircraft_path(aircraft))%></td>
    <td><%= link_to("Delete", "delete/"+aircraft.id.to_s)%></td>
  </tr>
<%end%>
</table>

